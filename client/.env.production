# Production Environment Variables for Vercel
# Backend Server URL - Update this with your deployed backend URL
VITE_BACKEND_URL=https://your-backend-url.vercel.app

# Socket.io Configuration
VITE_SOCKET_AUTO_CONNECT=true
VITE_SOCKET_RECONNECTION=true
VITE_SOCKET_RECONNECTION_ATTEMPTS=5
VITE_SOCKET_RECONNECTION_DELAY=1000

# App Configuration
VITE_APP_NAME=Flash Pulse Polls
VITE_APP_ENV=production
